.InputRange-slider
  appearance none
  background green-light
  border 1px solid green-light
  border-radius 100%
  cursor pointer
  display block
  height 1.2rem
  margin-left -0.5rem
  margin-top -0.85rem
  outline none
  position absolute
  top 50%
  transition -webkit-transform 0.3s ease-out, box-shadow 0.3s ease-out
  transition transform 0.3s ease-out, box-shadow 0.3s ease-out
  box-shadow 0px 12px 24px 0px rgba(0, 0, 0, 0.1)
  width 1.2rem
  .InputRange-slider:active
    transform scale(1.3)
  .InputRange-slider:focus
    box-shadow 0 0 0 5px rgba(63, 81, 181, 0.2)
  .InputRange.is-disabled .InputRange-slider
    background #cccccc
    border 1px solid #cccccc
    box-shadow none
    transform none

.InputRange-sliderContainer
  transition left 0.3s ease-out

.InputRange-label
  color #aaaaaa
  font-family "Helvetica Neue", san-serif
  font-size 1rem
  text-transform uppercase
  white-space nowrap

.InputRange-label--min,
.InputRange-label--max
  bottom -1.4rem
  position absolute
  display  none

.InputRange-label--min
  left 0

.InputRange-label--max
  right 0

.InputRange-label--value
  position absolute
  top 0.9rem

.InputRange-labelContainer
  left -50%
  position relative
  .InputRange-label--max .InputRange-labelContainer
    left 50%

.InputRange-track
  background #eeeeee
  border-radius 0.3rem
  display block
  height 0.4rem
  position relative
  transition left 0.3s ease-out, width 0.3s ease-out
  .InputRange.is-disabled .InputRange-track
    background #eeeeee

.InputRange-track--container
  left 0
  margin-top -0.15rem
  position absolute
  right 0
  top 50%

.InputRange-track--active
  background green-light

.InputRange
  cursor pointer
  height 1rem
  position relative
  width 100%
  margin-left auto
  margin-right auto
  
section.content-interval
  padding-top 27px